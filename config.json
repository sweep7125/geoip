{
    "input": [
        {
            "type": "v2rayGeoIPDat",
            "action": "add",
            "args": {
                "uri": "https://cdn.jsdelivr.net/gh/Loyalsoldier/v2ray-rules-dat@release/geoip.dat",
                "wantedList": [
                    "ru",
                    "private"
                ]
            }
        },
        {
            "type": "text",
            "action": "add",
            "args": {
                "name": "discord-voice-ip-list",
                "uri": "https://raw.githubusercontent.com/GhostRooter0953/discord-voice-ips/refs/heads/master/voice_domains/discord-voice-ip-list"
            }
        }
    ],
    "output": [
        {
            "type": "v2rayGeoIPDat",
            "action": "output",
            "args": {
                "outputDir": "./output",
                "outputName": "geoip.dat"
            }
        }
    ]
}
